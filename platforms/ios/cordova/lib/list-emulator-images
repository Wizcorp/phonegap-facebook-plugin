echo \""iPhone (Retina 4-inch)"\"

# this assumes Xcode 5 minimum not supported by ios-sim yet
# echo \""iPhone (Retina 4-inch 64-bit)"\"

echo \""iPhone"\"
echo \""iPad"\"
